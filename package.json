{
  "name": "clasp-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node build/index.js",
    "dev": "nodemon --watch src --exec 'ts-node' -r tsconfig-paths/register src/index.ts && stripe listen --forward-to localhost:7600/api/v1/order/handle-webhook",
    "typeorm": "typeorm-ts-node-commonjs -d src/database/data-source.ts",
    "migration:show": "npx typeorm-ts-node-commonjs migration:show -d ./src/database/data-source.ts",
    "migration:create": "node scripts/migration-create.js",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d ./src/database/data-source.ts",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d ./src/database/data-source.ts",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate -d ./src/database/data-source.ts -n",
    "db:reset": "ts-node scripts/reset-db.ts",
    "stripe": "stripe listen --forward-to localhost:7600/api/v1/order/handle-webhook"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/memory-cache": "^0.2.5",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.2.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "memory-cache": "^0.2.0",
    "nodemailer": "^6.9.14",
    "pg": "^8.12.0",
    "pug": "^3.0.3",
    "reflect-metadata": "^0.2.2",
    "stripe": "^16.1.0",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/nodemailer": "^6.4.15",
    "@types/pug": "^2.0.10",
    "@types/uuid": "^10.0.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4",
    "yargs": "^17.6.2"
  }
}
